create database Senales;
use Senales;
create table Senal(
`id` INT NOT NULL,
`Frecuencia_Central` FLOAT NOT NULL,
`Ancho_de_banda` FLOAT not null,
`Potencia_pico` FLOAT not null,
`Temp` FLOAT not null,
primary key (`id`)
);

ALTER TABLE `Senal`
  MODIFY `id` int NOT NULL AUTO_INCREMENT, AUTO_INCREMENT=1;
